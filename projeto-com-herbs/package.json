{
    "name": "DebtEngine",
    "version": "1.0.0",
    "description": "project generated by herbs-cli",
    "main": "src/index.js",
    "scripts": {
        "start": "node src/index.js",
        "dev": "npx nodemon src/index.js",
        "lint": "npx eslint \"**/*.{js,jsx}\" --quiet",
        "lint:fix": "npx eslint \"**/*.{js,jsx}\" --fix",
        "knex:make": "npx knex --knexfile knexFile.js migrate:make",
        "knex:migrate": "npx knex --knexfile knexFile.js migrate:latest",
        "knex:rollback": "npx knex --knexfile knexFile.js migrate:rollback",
        "knex:makeSeeds": "npx knex --knexfile knexFile.js seed:make",
        "knex:runSeeds": "npx knex --knexfile knexFile.js seed:run",
        "test": "npm run spec",
        "spec": "herbs spec"
    },
    "keywords": [
        "herbs"
    ],
    "author": "enderson",
    "license": "MIT",
    "dependencies": {
        "@herbsjs/herbarium": "^1.4.0",
        "@herbsjs/herbs": "^1.6.2",
        "@herbsjs/herbs2knex": "^1.5.7",
        "@herbsjs/herbs2rest": "^3.2.1",
        "@herbsjs/herbsshelf": "^4.0.1",
        "cors": "^2.8.5",
        "deepmerge": "^4.2.2",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "lodash.camelcase": "^4.3.0",
        "mocha": "^10.0.0",
        "multer": "^1.4.5-lts.1",
        "papaparse": "^5.4.1",
        "pg": "^8.7.3",
